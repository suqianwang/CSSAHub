<h1>Listing accounts</h1>
 
<table>
  <tr>
    <th>Email</th>
    <th>Username</th>
	<th>Name</th>
    <th>Password</th>
    <th></th>
  </tr>
 
  <% @accounts.each do |account| %>
    <tr>
      <td><%= account.email %></td>
      <td><%= account.username %></td>
	  <td><%= account.name %></td>
      <td><%= account.password %></td>
      <td><%= link_to 'Show', account_path(account) %></td>
    </tr>
  <% end %>
</table>